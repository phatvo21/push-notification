{
  "name": "push-notification",
  "version": "0.0.1",
  "description": "Service serves send push notifications",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "clean": "rm -r dist .db-data",
    "format": "prettier --write ./**/*.{ts,js,json,*rc}",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,html,js,json,jsx,md,ts,tsx}\" --fix --color",
    "lint:cache": "npm run lint --cache --quiet",
    "lint:fix": "npm run lint:cache --fix",
    "lint-staged": "lint-staged",
    "build:notification": "nest build notification",
    "start:notification": "cross-env SERVICE_NAME=notification nest start notification",
    "start:notification:dev": "npm run start:notification --watch",
    "start:notification:prod": "node dist/apps/notification/main",
    "test:e2e": "jest --forceExit --runInBand --detectOpenHandles",
    "test:compose": "docker-compose -f ./docker-compose.testing.yaml up -d && npm run test:e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:e2e"
    }
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "dependencies": {
    "@automapper/classes": "^4.2.0",
    "@automapper/core": "^4.2.0",
    "@automapper/nestjs": "^4.2.0",
