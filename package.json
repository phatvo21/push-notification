    "@nestjs/platform-fastify": "^8.0.2",
    "@nestjs/swagger": "^5.2.0",
    "@typegoose/typegoose": "^9.7.0",
    "child_process": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify-cookie": "^5.6.0",
    "fastify-csrf": "^3.1.0",
    "fastify-swagger": "^4.8.0",
    "i18next": "^20.6.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.6",
    "nanoid": "^3.3.1",
    "nestjs-i18n": "^8.1.6",
    "nestjs-pino": "^1.4.0",
    "nestjs-typegoose": "^7.1.38",
    "qs": "^6.10.3",
    "reflect-metadata": "^0.1.13",
